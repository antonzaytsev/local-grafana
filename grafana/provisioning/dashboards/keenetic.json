{
  "title": "Keenetic Router",
  "uid": "keenetic",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "1m",
  "timezone": "browser",
  "graphTooltip": 1,
  "time": { "from": "now-3h", "to": "now" },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "label": "Datasource",
        "type": "datasource",
        "pluginId": "prometheus",
        "current": {}
      },
      {
        "name": "router",
        "label": "Router",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "query": "label_values(keenetic_uptime_seconds, router)",
        "refresh": 2,
        "multi": false,
        "includeAll": false,
        "current": {}
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "CPU Load",
      "type": "stat",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "background",
        "graphMode": "area",
        "orientation": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 60 },
              { "color": "red", "value": 85 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "keenetic_cpu_load_percent{router=~\"$router\"}",
          "legendFormat": "CPU"
        }
      ]
    },
    {
      "id": 2,
      "title": "Memory Used",
      "type": "stat",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "background",
        "graphMode": "area"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "keenetic_memory_used_percent{router=~\"$router\"}",
          "legendFormat": "Memory"
        }
      ]
    },
    {
      "id": 3,
      "title": "Memory",
      "type": "gauge",
      "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "min": 0,
          "thresholds": {
            "mode": "percentage",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "keenetic_memory_used_bytes{router=~\"$router\"}",
          "legendFormat": "Used"
        },
        {
          "expr": "keenetic_memory_total_bytes{router=~\"$router\"}",
          "legendFormat": "Total"
        }
      ]
    },
    {
      "id": 4,
      "title": "Uptime",
      "type": "stat",
      "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "value",
        "graphMode": "none"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "dtdurations",
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "blue", "value": null }]
          }
        }
      },
      "targets": [
        {
          "expr": "keenetic_uptime_seconds{router=~\"$router\"}",
          "legendFormat": "Uptime"
        }
      ]
    },
    {
      "id": 5,
      "title": "CPU Load Over Time",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "options": {
        "tooltip": { "mode": "single" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": { "fillOpacity": 15, "lineWidth": 2 },
          "color": { "mode": "fixed", "fixedColor": "orange" }
        }
      },
      "targets": [
        {
          "expr": "keenetic_cpu_load_percent{router=~\"$router\"}",
          "legendFormat": "CPU %"
        }
      ]
    },
    {
      "id": 6,
      "title": "Memory Over Time",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 4, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["last", "max"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "min": 0,
          "custom": { "fillOpacity": 10, "lineWidth": 2 }
        }
      },
      "targets": [
        {
          "expr": "keenetic_memory_used_bytes{router=~\"$router\"}",
          "legendFormat": "Used"
        },
        {
          "expr": "keenetic_memory_free_bytes{router=~\"$router\"}",
          "legendFormat": "Free"
        },
        {
          "expr": "keenetic_memory_total_bytes{router=~\"$router\"}",
          "legendFormat": "Total"
        }
      ]
    }
  ]
}
