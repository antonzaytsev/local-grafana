# Ports (host-side, all configurable)
VM_PORT=8428
VMAGENT_PORT=8429
GRAFANA_PORT=8430
DOCS_PORT=8431

# Retention for all data (raw + aggregated live in same instance).
# Raw metrics: useful detail for recent data; VictoriaMetrics compresses older data automatically.
# Aggregated metrics (*:1h_avg etc.) provide efficient long-range queries for the full period.
# Recommended: 90d for mini PC. Increase if disk allows (VM compresses ~10x vs raw).
VM_RETENTION=90d

# Grafana credentials
GRAFANA_USER=admin
GRAFANA_PASSWORD=admin

# Keenetic router
KEENETIC_HOST=192.168.1.1
KEENETIC_LOGIN=admin
KEENETIC_PASSWORD=
